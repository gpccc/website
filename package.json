{
  "name": "gp-website",
  "version": "2.0.0",
  "description": "GPCCC.org website redesign",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run test && npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest",
    "test:CI": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll",
    "open:cover": "npm run test:cover && open-cli ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Sisters and brothers at GPCCC",
  "license": "MIT",
  "dependencies": {
    "@hot-loader/react-dom": "17.0.0",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "connected-react-router": "6.8.0",
    "i18next": "19.8.4",
    "i18next-browser-languagedetector": "6.0.1",
    "js-cookie": "2.2.1",
    "object-assign": "4.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.7.3",
    "react-redux": "7.2.2",
    "react-resize-detector": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/node": "7.12.6",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-react-constant-elements": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.5",
    "acorn": "8.0.4",
    "ajv": "6.12.6",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.13",
    "chalk": "4.1.0",
    "concurrently": "5.3.0",
    "connect-history-api-fallback": "1.6.0",
    "coveralls": "3.1.0",
    "css-loader": "5.0.1",
    "cssnano": "4.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-watch": "7.0.0",
    "file-loader": "6.2.0",
    "hard-source-webpack-plugin": "0.13.1",
    "history": "5.0.0",
    "html-webpack-plugin": "4.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "1.3.1",
    "mockdate": "3.0.2",
    "node-sass": "5.0.0",
    "open-cli": "6.0.1",
    "postcss-loader": "4.0.4",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "17.0.1",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.4",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-dev-middleware": "4.0.2",
    "webpack-hot-middleware": "2.25.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  }
}
